datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Source {
  facebook
  tiktok
}

enum FunnelStage {
  top
  bottom
}

model Event {
  eventId      String       @id
  timestamp    DateTime
  source       Source
  funnelStage  FunnelStage
  eventType    String
  data         Json
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
}
